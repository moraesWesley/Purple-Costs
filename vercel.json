{
  "version": 2,
  "builds": [
    {
      "src": "api/server.js",
      "use": "@vercel/node",
      "config" :{
        "includeFiles" : ["db.json"]
      }
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
        "src": "/api/(.*)",
        "dest": "/api/server.js"
      }
  ]
}
